

<form action="/users" method="post" accept-charset="UTF-8">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  Username: <input type="text" label="username" name="user[username]"><br/>
  Email: <input type="text" label="email" name="user[email]"><br/>
  Password: <input type="password" label="password" name="user[password]"></br>
  <input type="submit" value="Submit">
</form>
